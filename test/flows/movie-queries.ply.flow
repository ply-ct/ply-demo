attributes:
  NodeStyle: BoxIcon
  display: 'x=23,y=18,w=156,h=27'
steps:
  - id: s1
    name: Start
    path: start
    attributes:
      display: 'x=56,y=97,w=60,h=40'
    links:
      - id: l1
        attributes:
          display: 'type=Elbow,x=154,y=117,xs=120&186,ys=117&117'
        to: s3
  - id: s2
    name: Stop
    path: stop
    attributes:
      display: 'x=720,y=100,w=60,h=40'
  - id: s3
    name: |-
      Movies by
      Year & Rating
    path: request.ts
    attributes:
      display: 'x=190,y=90,w=100,h=60'
      method: GET
      url: '${baseUrl}?year=${year}&rating=${rating}'
      headers: '[["Accept","application/json"]]'
    links:
      - id: l2
        to: s4
        type: link
        attributes:
          display: 'type=Elbow,x=332,y=120,xs=294&366,ys=120&120'
    type: step
  - id: s4
    name: |-
      Movie by
      ID
    path: request.ts
    attributes:
      display: 'x=370,y=90,w=100,h=60'
      method: GET
      url: '${baseUrl}/${@["Movies by Year & Rating"].response.body.movies[0].id}'
      headers: '[["Accept","application/json"]]'
    links:
      - id: l3
        to: s5
        type: link
        attributes:
          display: 'type=Elbow,x=513,y=120,xs=474&546,ys=120&120'
    type: step
  - id: s5
    name: |-
      Great Films
      of 1935
    path: request.ts
    attributes:
      display: 'x=550,y=90,w=100,h=60'
      method: GET
      url: '${baseUrl}?${queries.highlyRated1935}'
      headers: '[["Accept","application/json"]]'
    links:
      - id: l4
        to: s2
        type: link
        attributes:
          display: 'type=Elbow,x=685,y=120,xs=654&716,ys=120&120'
    type: step
