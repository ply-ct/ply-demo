attributes:
  NodeStyle: BoxIcon
  display: x=23,y=18,w=156,h=27
steps:
  - id: s1
    name: Start
    path: start
    attributes:
      display: x=40,y=70,w=60,h=40
    links:
      - id: l1
        to: s3
        attributes:
          display: type=Elbow,x=127,y=90,xs=104&146,ys=90&90
  - id: s2
    name: Stop
    path: stop
    attributes:
      display: x=490,y=70,w=60,h=40
  - id: s3
    name: |-
      Get
      Movie
    path: request
    links:
      - id: l4
        to: s4
        attributes:
          display: type=Elbow,x=290,y=90,xs=254&326,ys=90&90
    attributes:
      display: x=150,y=50,w=100,h=80
      url: ${baseUrl}/movies?title=The Invisible Man
      method: GET
      headers: '[["Accept","application/json"]]'
  - id: s4
    name: Subflow
    path: subflow
    links:
      - id: l3
        to: s2
        attributes:
          display: type=Elbow,x=463,y=90,xs=434&486,ys=90&90
    attributes:
      display: x=330,y=50,w=100,h=80
      subflow: test/flows/movie-by-id.ply.flow
      values: '[["movieId","${@s3.response.body.movies[0].id}"]]'
